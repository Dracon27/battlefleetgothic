        <selectionEntryGroup id="89f5-a5b2-5227-90e3" name="Ordnance" hidden="false" collective="false" import="true" defaultSelectionEntryId="e705-f61b-0b0b-ebbf">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="4016-2c38-b268-0060" type="min"/>
