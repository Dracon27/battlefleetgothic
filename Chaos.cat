    <selectionEntry id="19fa9a3b-ce7a-4a39-8137-b9755c7f57eb" name="Abaddon the Despolier" publicationId="1bc8-5968-21c3-0f27" page="43" hidden="false" collective="false" import="true" type="upgrade">
